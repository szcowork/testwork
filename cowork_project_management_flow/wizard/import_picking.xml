<?xml version="1.0"?>
<odoo>
	<data>

		<record id="picking_line_wizard_view" model="ir.ui.view">
			<field name="name">导入调拨清单</field>
			<field name="model">import.picking.line.wizard</field>
			<field name="arch" type="xml">
				<form>
					<group>
					    <field name="import_option" widget="radio"/>
						<field name="picking_file"/>
					</group>
					<footer>
						<button string="导入" name="import_picking" class="oe_highlight" type="object"/>
						<button string="取消" class="btn-default" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="import_picking_line_action" model="ir.actions.act_window">
			<field name="name">导入调拨清单</field>
			<field name="res_model">import.picking.line.wizard</field>
			<field name="target">new</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
		</record>

    </data>
</odoo>