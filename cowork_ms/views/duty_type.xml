<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="view_form_duty_type">
        <field name="name">请假类型</field>
        <field name="model">duty.type</field>
        <field name="arch" type="xml">
            <form string="请假类型">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="view_tree_duty_type">
        <field name="name">请假类型</field>
        <field name="model">duty.type</field>
        <field name="arch" type="xml">
            <tree string="请假类型">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_duty_type">
        <field name="name">请假类型</field>
        <field name="res_model">duty.type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
